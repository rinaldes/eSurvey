<% params[:cancel] ||= false %>
<% unless params[:cancel] %>
	$("#set-logical-form").html("<%= j(render '/wizards/set_logical', id: params[:id]) %>")
	set_hide_button()
	$('html, body').animate({
    scrollTop: $("#set-logical-form").offset().top
	}, 2000);
<% else %>
	$("#set-logical-form").html("")
<% end %>